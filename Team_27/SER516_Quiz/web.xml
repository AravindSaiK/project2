<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>Quiz</display-name>
  <description>SER516 Quiz</description>
  
  <servlet>
    <servlet-name>QuizDetailsController</servlet-name>
    <servlet-class>edu.asu.ser516.quiz.controller.QuizDetailsController</servlet-class>
  </servlet>

<servlet>
    <servlet-name>QuizIntroController</servlet-name>
    <servlet-class>edu.asu.ser516.quiz.controller.QuizIntroController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteQuizController</servlet-name>
    <servlet-class>edu.asu.ser516.quiz.controller.DeleteQuizController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QuizDetailsController</servlet-name>
    <url-pattern>/createQuiz</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>QuizIntroController</servlet-name>
    <url-pattern>/getQuiz</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>DeleteQuizController</servlet-name>
    <url-pattern>/deleteQuiz</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
   <welcome-file>index.html</welcome-file>
 </welcome-file-list>

</web-app>
